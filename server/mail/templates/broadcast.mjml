<mjml>
    <mj-head>
        <mj-attributes>
            <mj-text font-size="14px"/>
            <mj-all font-family="'Helveticaa Neue', 'Arial Nova', 'Nimbus Sans', sans-serif"/>
        </mj-attributes>
    </mj-head>

    <mj-body background-color="#1bb17a">
        <mj-section padding-top="30px">
            <mj-column background-color="white" border-radius="20px" padding-bottom="10px">
                <mj-image width="100px" src="https://cslabs.be/wp-content/uploads/2024/01/logo-CSLabs.png"/>

                <mj-text>
                    <h1 align="center" style="font-size: 24px">
                        {{title}}
                    </h1>
                    <p>
                        {{body}}
                    </p>
                </mj-text>

                <mj-divider border-width="1px" border-color="lightgray"/>

                <mj-social icon-size="28px" mode="horizontal">
                    <mj-social-element href="https://cslabs.be/"
                                       src="data:image/svg+xml;base64,"/>
                    <mj-social-element href="https://www.facebook.com/ComputerScienceLabs"
                                       src="data:image/svg+xml;base64,"/>
                    <mj-social-element href="https://www.instagram.com/cslabs_namur"
                                       src="data:image/svg+xml;base64,"/>
                    <mj-social-element href="https://www.linkedin.com/company/cslabs-namur"
                                       src="data:image/svg+xml;base64,"/>
                    <mj-social-element href="https://x.com/CSLabs"
                                       src="data:image/svg+xml;base64,"/>
                    <mj-social-element href="https://www.youtube.com/@CSLabs_Namur"
                                       src="data:image/svg+xml;base64,"/>
                </mj-social>
            </mj-column>
        </mj-section>
    </mj-body>
</mjml>
